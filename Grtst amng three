#include<stdio.h>
main()
{
    int a,b,c;
    printf("\n Enter the three values: ");
    scanf("%d%d%d",&a,&b,&c);
    if(a==b && b==c)
     printf("\n All three are Equal");
    else if(a>b && a>c)
     printf("\n %d is Greater",a);
    else if(b>c)
     printf("\n %d is Greater",b);
    else
     printf(" \n %d is greater ",c);
}