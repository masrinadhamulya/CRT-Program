#include<stdio.h>
main()
{
    int i,n,count;
    count=0;
    printf("\nEnter a Number ");
    scanf("%d",&n);
    for(i=2;i<=n/2;i++)
    {
        if(n%i==0)
        count=count+1;   
    }
    if(count==0)
     printf("\n%d is a Prime No.",n);
    else
     printf("\n%d is Not a prime No.",n);
}