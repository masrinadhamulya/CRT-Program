#include<stdio.h>
main()
{
    int i,n,count;
    count=0;
    printf("\nEnter a Number ");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        if(n%i==0)
        count=count+1;   
    }
    if(count==2)
     printf("\n%d is a Prime No. and for %d the no.of factors are %d",n,n,count);
    else
     printf("\n%d is Not a prime and for %d the no.of Factors are %d",n,n,count);
}